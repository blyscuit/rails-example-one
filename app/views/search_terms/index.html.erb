<div class="w-full">
  <% if notice.present? %>
    <p class="py-2 px-3 bg-green-50 mb-5 text-green-500 font-medium rounded-lg inline-block" id="notice"><%= notice %></p>
  <% end %>

  <%= render "import" %>

  <%= @search_terms.length %>
  <div id="search_terms" class="min-w-full">
    <%= render @search_terms %>
  </div>
</div>
